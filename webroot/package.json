{
    "name": "vue-admin-bd",
    "version": "4.2.1",
    "smallVersion": "4.2.3.3",
    "description": "A vue admin template with Element UI",
    "author": "Pigeon",
    "license": "MIT",
    "scripts": {
        "dev": "vue-cli-service serve --open",
        "build": "vue-cli-service build --mode test",
        "build:prd": "vue-cli-service build --mode prd",
        "build:pet": "vue-cli-service build --mode pet",
        "build:uat": "vue-cli-service build --mode uat",
        "build:k8s": "vue-cli-service build --mode k8s",
        "build:stage": "vue-cli-service build --mode staging",
        "report": "npm run build --report",
        "preview": "node build/index.js --preview",
        "lint": "eslint --ext .js,.vue src",
        "test:unit": "jest --clearCache && vue-cli-service test:unit",
        "test:ci": "npm run lint && npm run test:unit",
        "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
        "precommit": "lint-staged"
    },
    "dependencies": {
        "@babel/plugin-proposal-optional-chaining": "^7.11.0",
        "axios": "0.18.1",
        "babel-polyfill": "^6.26.0",
        "crypto-js": "^3.1.9-1",
        "d3": "^5.1.0",
        "dagre-d3": "^0.6.4",
        "dayjs": "^1.11.7",
        "echarts": "^4.9.0",
        "echarts-gl": "^1.1.2",
        "echarts-liquidfill": "^2.0.2",
        "echarts-wordcloud": "^1.1.3",
        "element-tiptap": "^1.27.1",
        "element-ui": "2.13.0",
        "highlight.js": "^11.11.1",
        "js-base64": "^3.7.2",
        "jsencrypt": "^3.2.0",
        "normalize.css": "7.0.0",
        "nprogress": "0.2.0",
        "path-to-regexp": "2.4.0",
        "pdfjs-dist": "^2.5.207",
        "qs": "^6.9.1",
        "screenfull": "^5.1.0",
        "spark-md5": "^3.0.2",
        "sql-formatter": "^4.0.2",
        "v-viewer": "^1.5.1",
        "vee-validate": "^2.0.0-rc.21",
        "vue": "2.6.10",
        "vue-clipboard2": "^0.3.3",
        "vue-codemirror": "^4.0.6",
        "vue-count-to": "^1.0.13",
        "vue-draggable-resizable-gorkys": "^2.4.4",
        "vue-i18n": "^8.7.0",
        "vue-markdown": "^2.2.4",
        "vue-pdf": "^4.2.0",
        "vue-resize": "^1.0.1",
        "vue-router": "3.1.0",
        "vue2-ace-editor": "^0.0.15",
        "vuedraggable": "^2.24.1",
        "vuex": "3.1.0"
    },
    "devDependencies": {
        "@babel/core": "7.0.0",
        "@babel/register": "7.0.0",
        "@vue/cli-plugin-babel": "3.6.0",
        "@vue/cli-plugin-eslint": "^3.9.1",
        "@vue/cli-plugin-unit-jest": "3.6.3",
        "@vue/cli-service": "3.6.0",
        "@vue/test-utils": "1.0.0-beta.29",
        "autoprefixer": "^9.5.1",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "8.0.1",
        "babel-jest": "23.6.0",
        "chalk": "2.4.2",
        "compression-webpack-plugin": "^4.0.0",
        "connect": "3.6.6",
        "core-js": "^2.6.12",
        "eslint": "5.15.3",
        "eslint-plugin-vue": "5.2.2",
        "handlebars": "^4.5.3",
        "html-webpack-plugin": "3.2.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.3.5",
        "node-sass": "^4.9.0",
        "runjs": "^4.3.2",
        "sass-loader": "^7.1.0",
        "script-ext-html-webpack-plugin": "2.1.3",
        "script-loader": "0.7.2",
        "serve-static": "^1.13.2",
        "style-loader": "^1.0.0",
        "svg-sprite-loader": "4.1.3",
        "svgo": "1.2.2",
        "svgo-loader": "^2.2.1",
        "vue-template-compiler": "2.6.10",
        "webpack-bundle-analyzer": "^3.8.0",
        "vue-shell": "^1.0.3"
    },
    "gitHooks": {
        "pre-commit": ""
    },
    "lint-staged": {
        "**/*.{js,json,css,vue}": [
            "eslint --fix",
            "git add"
        ]
    },
    "engines": {
        "node": ">=8.9",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "IE 9"
    ]
}
